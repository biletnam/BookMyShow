<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>current_movie</servlet-name>
        <servlet-class>current_movie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>emp_submit1</servlet-name>
        <servlet-class>emp_submit1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>movie_time</servlet-name>
        <servlet-class>movie_time</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>emp_reg</servlet-name>
        <servlet-class>emp_reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_movie</servlet-name>
        <servlet-class>update_movie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_current_movie</servlet-name>
        <servlet-class>update_current_movie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>emp_update</servlet-name>
        <servlet-class>emp_update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_emp1</servlet-name>
        <servlet-class>update_emp1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>current_movie</servlet-name>
        <url-pattern>/current_movie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>emp_submit1</servlet-name>
        <url-pattern>/emp_submit1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>movie_time</servlet-name>
        <url-pattern>/movie_time</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>emp_reg</servlet-name>
        <url-pattern>/emp_reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_movie</servlet-name>
        <url-pattern>/update_movie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_current_movie</servlet-name>
        <url-pattern>/update_current_movie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>emp_update</servlet-name>
        <url-pattern>/emp_update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_emp1</servlet-name>
        <url-pattern>/update_emp1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
